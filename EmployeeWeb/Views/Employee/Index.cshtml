@model IEnumerable<Employee>

<table class="table">
    <thead>
        <tr>
            <th>Id</th>
            <th>First Name</th>
            <th>Last Name</th>
            <th>City</th>
            <th>İşlemler</th>
        </tr>
    </thead>
    <tbody>
        @foreach(var item in Model)
        {
            <tr>
                <td>@item.Id</td>
                <td>@item.FName</td>
                <td>@item.LName</td>
                <td>@item.City</td>
                <td>
                    <a class="btn btn-outline-primary" asp-controller="Employee" asp-action="Edit" asp-route-id="@item.Id"><i class="bi bi-pencil-square"></i>Güncelle</a> |
                    <a class="btn btn-outline-primary" asp-controller="Employee" asp-action="Details" asp-route-id="@item.Id"><i class="bi bi-eye"></i>Detay</a> |
                    <a class="btn btn-outline-primary" asp-controller="Employee" asp-action="Delete" asp-route-id="@item.Id"><i class="bi bi-trash"></i>Sil</a>
                </td>
            </tr>
        }

    </tbody>



</table>